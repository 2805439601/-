
.filter-shadow{
	position: fixed;
	width: 100%;
	top: 0;
	bottom: 0;
	z-index: 3;
	background: rgba(0,0,0,.5);
}
.jr{
  border-radius:24rpx 24rpx 0px 0px;
  width: 100%;
  padding: 36rpx 32rpx 0 32rpx;
  box-sizing: border-box;
  z-index: 3;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
}
.jr_sp{
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
  position: relative;
}
.gb{
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 0;
  right: 0;
  
}
.jr_img{
  width: 244rpx;
  height: 244rpx;
  background-image: url(http://test.api.meibohate.com/images/square.png);
  background-size: cover;
  border-radius:12rpx;
  overflow: hidden;
  margin-right: 18rpx;

}
.jr_nra{
  font-size:24rpx;
  font-weight:500;
  line-height:56rpx;
  color:rgba(241,64,75,1);
}
.jr_nra text{
  font-size:40rpx;
}
.jr_nrc{
  margin-bottom: 8rpx;
  font-size:24rpx;
  font-weight:400;
  line-height:34rpx;
  color:rgba(153,153,153,1);
}
.jr_nrb{
  font-size:28rpx;
  font-weight:400;
  line-height:40rpx;
  color:rgba(51,51,51,1);
}
.jr_nrb text{
  margin-right: 12rpx;
}
.jr_kd{
  padding: 30rpx 0 28rpx 0;
  display: flex;
  /* border-bottom:1px solid rgba(242,242,242,1); */
  color:rgba(102,102,102,1);
}
.jr_kd button{
  font-size:28rpx;
  font-weight:400;
  line-height:40rpx;
  padding: 20rpx 58rpx 20rpx 60rpx;
  margin: 0 30rpx 0 0;
}
.jr_kdbut{
  background:rgba(248,248,248,1);
  border-radius:40rpx;
}
.jr_akd{
  color:rgba(241,64,75,1);
  background:rgba(255,241,241,1);
  border:1rpx solid rgba(241,64,75,1);
}
.jr_bkd{
  background:rgba(248,248,248,1);
}
.jr_xzd{
  display: flex;
  font-size:28rpx;
  font-weight:500;
  line-height:40rpx;
  color:rgba(51,51,51,1);
  margin-bottom: 46rpx;
}
.jr_ximg{
  width: 24rpx;
  height: 24rpx;
  margin-right: 12rpx;
}
.jr_xdz{
  width: 600rpx;
}



.skuBox_class text{
  font-size:24rpx;
  font-weight:500;
  line-height:34rpx;
  color:rgba(51,51,51,1);
}
.skuBox_class view{
  margin-top: 34rpx;
  color:rgba(102,102,102,1);
}
.skuBox_class view button{
  display: inline-block;
  font-size:28rpx;
  font-weight:400;
  line-height:40rpx;
  padding: 10rpx 32rpx;
  border-radius:40rpx;
  margin-right: 24rpx;
}



.typeStyle {
  /* margin-top: 34rpx; */
  display: flex;
  flex-wrap: wrap ;
}
.textStyle view{
   box-sizing: border-box;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
}

.outStyle {
  width: 100%;
  display: flex;
  /* flex-direction: column; */
  justify-content: space-between;
  align-items: center;
  margin-top: 60rpx;
}

.outStyleText {
  font-size:24rpx;
  font-weight:500;
  line-height:34rpx;
  color:rgba(51,51,51,1)
}


.sl{
  display: flex;
}
.sl_j{
  width:66rpx;
  height:60rpx;
  background:rgba(250,250,250,1);
  font-size:48rpx;
  font-weight:100;
  line-height:60rpx;
  color:rgba(153,153,153,1);
  text-align: center;
}
.sl .sl_j:nth-child(1){
  border-radius:30rpx 0rpx 0rpx 30rpx;
}
.sl view:nth-child(3){
  border-radius:0px 30px 30px 0px;
}
.sl_s{
  height:60rpx;
  background:rgba(248,248,248,1);
  font-size:36rpx;
  font-weight:500;
  line-height:60rpx;
  color:rgba(51,51,51,1);
  text-align: center;
  width: 76rpx;
  margin: 0 2rpx;
}
.qd{
  margin: 49rpx 0 25rpx 0;
}
.qd button{
  font-size:32rpx;
  font-weight:400;
  line-height:44rpx;
  color:rgba(255,255,255,1);
  background:linear-gradient(90deg,rgba(255,93,105,1) 0%,rgba(240,64,74,1) 100%);
  border-radius:88rpx;
  padding: 22rpx 0;
}
.productActive{
  box-sizing: border-box;
  background:rgba(255,241,241,1);
  border:1rpx solid rgba(241,64,75,1);
  color:rgba(241,64,75,1);
  border-radius:40rpx;
}
.noneActive{
 color:rgba(102,102,102,1);
 background:rgba(248,248,248,1);
 pointer-events: none;
}
.navpa{
  padding-bottom: 120rpx;
}
.navTop{
  position: fixed;
  top: 0;
  left: 0;
}
.guoqi{
  width: 100%;
  text-align: center;
  background:rgba(255,255,255,1);
  padding-bottom:  50rpx;
}
.guoqi_img{
  width: 260rpx;
  height: 260rpx;
  display: inline-block;
}
.guoqi_text{
  font-size:28rpx;
  font-family:PingFang SC;
